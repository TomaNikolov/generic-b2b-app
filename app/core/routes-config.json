{
    "tabs": [
        {
            "path": "",
            "redirectTo": "/(homeTab:home/home//galleryTab:gallery)",
            "pathMatch": "full"
        },
        {
            "path": "home",
            "outlet": "homeTab",
            "loadChildren": "./home#HomeModule"
        },
        {
            "path": "gallery",
            "outlet": "galleryTab",
            "loadChildren": "./gallery#GalleryModule"
        }
    ],
    "modals": [
        {
            "path": "profile",
            "loadChildren": "./profile#ProfileModule",
            "title": "Profile"
        },
        {
            "path": "terms",
            "loadChildren": "./terms#TermsModule",
            "title": "Terms and Conditions"
        },
        {
            "path": "master",
            "title": "Master detail",
            "loadChildren": "./master-details/customers.module#CustomersModule"
        },
        {
            "path": "my-customers",
            "title": "My Customers",
            "loadChildren": "./my-customers/my-customers.module#MyCustomersModule"
        },

        {
            "path": "login",
            "loadChildren": "./login/login.module#LoginModule",
            "title": "login"
        },
        {
            "path": "reports",
            "title": "Reports",
            "loadChildren": "./reports/reports.module#ReportsModule"
        },
        {
            "path": "place-order",
            "title": "Place Order",
            "loadChildren": "./place-order/place-order.module#PlaceOrderModule"
        }
    ]
}
